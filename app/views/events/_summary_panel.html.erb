<%= content_tag :h4, "Data" %>
<%= content_tag :ul, :class =>"nav nav-tabs" do %>
  <%= content_tag :li, :class => "active" do %><%= link_to "Statistics", "#statistics", :"data-toggle" => "tab" %><% end %>
  <%= content_tag :li do %><%= link_to "Videos", "#videos", :"data-toggle" => "tab" %><% end %>
  <%= content_tag :li do %><%= link_to "Images", "#images", :"data-toggle" => "tab" %><% end %>
<% end %> <!-- nav nav-tabs -->

<%= content_tag :div, :class => "tab-content" do %>
  <%= content_tag :div, :class => "tab-pane active", :id => "statistics" do %>
    <%= render 'statistics_panel' %>
  <% end %> <!-- statistics -->
  <%= content_tag :div, :class => "tab-pane", :id => "videos" do %>
    <%= render 'videos_panel' %>
  <% end %> <!-- videos -->
  <%= content_tag :div, :class => "tab-pane", :id => "images" do %>
    <%= render 'images_panel' %>
  <% end %> <!-- images -->
<% end %> <!-- tab-content -->
