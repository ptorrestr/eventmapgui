  <% params[:tweets] = true %>
  <% params.delete :statistics %>
  <% params.delete :videos %>
  <% params.delete :images %>
  $("#tweetsPanel").html("<%= escape_javascript(render("tweets_panel")) %>");
<% end %>
<% if @statistics_flag %>
  <% params[:statistics] = true %>
